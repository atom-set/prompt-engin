#!/usr/bin/env python3
"""
Prompt Engine CLI 包装脚本

如果项目未安装，可以使用此脚本直接运行 CLI。
"""

import sys
from pathlib import Path

# 添加项目根目录到路径
project_root = Path(__file__).parent.parent
sys.path.insert(0, str(project_root / "src"))

# 导入并运行 CLI
from prompt_engine.cli import main

if __name__ == "__main__":
    main()

